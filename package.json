  "keywords": [
    "javascript",
    "redux",
    "middleware",
    "api",
    "token",
    "jwt"
  ],
  "author": "Edward Stone <edwardjstone@yahoo.com>",
  "repository": "https://github.com/eadmundo/redux-token-api-middleware",
  "bugs": {
    "url": "https://github.com/eadmundo/redux-token-api-middleware/issues"
  },
  "homepage": "https://github.com/eadmundo/redux-token-api-middleware",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.7.0",
    "lodash.isarraylikeobject": "^4.2.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isundefined": "^3.0.1",
    "lodash.map": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "lodash.startswith": "^4.2.1",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.2",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "eslint": "~2.2.0",
    "html-webpack-plugin": "^2.14.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1",
    "webpack": "^1.12.14"
  },
  "npmName": "redux-api-token-middleware",
